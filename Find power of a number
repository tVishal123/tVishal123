import java.util.Scanner;
public class Solution {
    
    public static void main(String[] args) {
        
         Scanner sc = new Scanner(System.in);
        int b = sc.nextInt();
        int p = sc.nextInt();
        int res =1;
        for(int i=0; i<p;i++)
        {
            res *=b;
        }
        System.out.print(res);
    }
}
